{
  "Import Rax": {
    "prefix": "imc",
    "body": ["import { createElement } from 'rax';"],
    "description": "Import Rax"
  },
  "Import { Component }": {
    "prefix": "imcc",
    "body": ["import { createElement, Component } from 'rax';"],
    "description": "Import { createElement, Component }"
  },
  "Import View": {
    "prefix": "imv",
    "body": ["import View from 'rax-view';"],
    "description": "Import View"
  },
  "Import Text": {
    "prefix": "imt",
    "body": ["import Text from 'rax-text';"],
    "description": "Import Text"
  },
  "Import All About Rax": {
    "prefix": "ima",
    "body": [
      "import { createElement, Component } from 'rax';",
      "import Text from 'rax-text';",
      "import View from 'rax-view';"
    ],
    "description": "Import All About Rax"
  },
  "Class Component": {
    "prefix": "cc",
    "body": [
      "class $1 extends Component {",
      "\tstate = { $2 }",
      "\trender() { ",
      "\t\treturn ( $0 );",
      "\t}",
      "}",
      " ",
      "export default $1;"
    ],
    "description": "Class Component"
  },
  "Rax Class Template": {
    "prefix": "rct",
    "body": [
      "import { createElement, Component } from 'rax';",
      "import Text from 'rax-text';",
      "import View from 'rax-view';",
      "\nclass $1 extends Component {",
      "\tstate = { $2 }",
      "\trender() { ",
      "\t\treturn ( $0 );",
      "\t}",
      "}",
      " ",
      "export default $1;"
    ],
    "description": "Rax Class Component"
  },
  "Rax Function Template": {
    "prefix": "rft",
    "body": [
      "import { createElement } from 'rax';",
      "import Text from 'rax-text';",
      "import View from 'rax-view';",
      "\nfunction $1()  {",
      "\t\treturn ( $0 );",
      "}",
      "export default $1;"
    ],
    "description": "Rax Function Template"
  },
  "Higher Order Component": {
    "prefix": "hoc",
    "body": [
      "function $1($2) {",
      "\treturn class extends Component {",
      "\t\tconstructor(props) {",
      "\t\t\tsuper(props);",
      "\t\t}",
      " ",
      "\t\trender() {",
      "\t\t\treturn <$2 {...this.props} $0/>",
      "\t\t}",
      "\t}",
      "}"
    ],
    "description": "Higher Order Component"
  },
  "View": {
    "prefix": "view",
    "body": ["<View>$1</View>"],
    "description": "View"
  },
  "Text": {
    "prefix": "text",
    "body": ["<Text>$1</Text>"],
    "description": "Text"
  }
}
